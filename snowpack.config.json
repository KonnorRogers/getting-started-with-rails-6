{
  "installOptions": {
    "NODE_ENV": true
  },
  "scripts": {
    "mount:javascript": "mount app/javascript/packs --to /javascripts/javascript",
    "mount:stylesheets": "mount app/javascript/stylesheets --to /stylesheets/javascript",
    "build:css": "postcss"
  },
  "devOptions": {
    "port": 3035,
    "open": "none",
    "out": "javascript"
  },
  "proxy": {
    "0.0.0.0:3035": "0.0.0.0:3035"
  },
  "buildOptions": {
    "clean": true,
    "baseUrl": "/"
  },
  "plugins": [
    "@snowpack/plugin-babel",
    "@snowpack/plugin-parcel"
  ]
}
