{
  "installOptions": {
    "NODE_ENV": true
  },
  "scripts": {
    "mount:javascript": "mount app/javascript/packs --to /js",
    "mount:stylesheets": "mount app/javascript/stylesheets --to /css"
  },
  "devOptions": {
    "port": 3035,
    "open": "none",
    "out": "_frontend_"
  },
  "proxy": {
    "0.0.0.0:3035": "0.0.0.0:3035"
  },
  "buildOptions": {
    "clean": true,
    "baseUrl": "/"
  },
  "plugins": [
    "@snowpack/plugin-babel"
  ]
}
